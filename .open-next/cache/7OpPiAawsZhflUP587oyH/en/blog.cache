\":\"h2\"},{\"_key\":\"5xtatv\",\"_type\":\"block\",\"children\":[{\"_key\":\"7pajn7\",\"_type\":\"span\",\"marks\":[],\"text\":\"Create blocks using slash commands. Try typing \"},{\"_key\":\"6nj8rl\",\"_type\":\"span\",\"marks\":[\"code\"],\"text\":\"/\"},{\"_key\":\"3nj0vk\",\"_type\":\"span\",\"marks\":[],\"text\":\" (the slash character) to see available options. Type \"},{\"_key\":\"7hams\",\"_type\":\"span\",\"marks\":[\"strong\",\"code\"],\"text\":\"/instruction \"},{\"_key\":\"dm0fgb\",\"_type\":\"span\",\"marks\":[],\"text\":\"to give Canvas a task to complete, like writing a conclusion paragraph or summarizing the key points.\"}],\"markDefs\":[],\"style\":\"normal\"},{\"_key\":\"5xtatw\",\"_type\":\"block\",\"children\":[{\"_key\":\"g4udev\",\"_type\":\"span\",\"marks\":[],\"text\":\"Here's an instruction you can run by pushing the play button ▶️:\"}],\"markDefs\":[],\"style\":\"normal\"},{\"_key\":\"5xtatx\",\"_type\":\"block\",\"children\":[{\"_key\":\"4d1vu4\",\"_type\":\"span\",\"marks\":[\"strong\",\"code\"],\"text\":\"Pro-tip\"},{\"_key\":\"fpuglb\",\"_type\":\"span\",\"marks\":[],\"text\":\" Trigger slash commands at the beginning of existing blocks to change their type:\"}],\"markDefs\":[],\"style\":\"normal\"},{\"_key\":\"5xtaty\",\"_type\":\"block\",\"children\":[{\"_key\":\"6508nm\",\"_type\":\"span\",\"marks\":[],\"text\":\"Try using the slash command to change this block from a blockquote to a normal paragraph.\"}],\"markDefs\":[],\"style\":\"blockquote\"},{\"_key\":\"5xtatz\",\"_type\":\"block\",\"children\":[{\"_key\":\"70a255\",\"_type\":\"span\",\"marks\":[],\"text\":\"Content Mapping\"}],\"markDefs\":[],\"style\":\"h2\"},{\"_key\":\"5xtau0\",\"_type\":\"block\",\"children\":[{\"_key\":\"7ozqe1\",\"_type\":\"span\",\"marks\":[\"em\"],\"text\":\"Content Mapping\"},{\"_key\":\"41r31b\",\"_type\":\"span\",\"marks\":[],\"text\":\" lets you convert your Canvas documents into structured content that lifts right into Sanity Studio. It automatica